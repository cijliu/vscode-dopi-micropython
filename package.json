{
	"name": "dopi-micropython",
	"displayName": "Dopi",
	"publisher": "cijliu",
	"description": "",
	"version": "0.0.4",
	"icon": "images/dopi.png",

	"engines": {
		"vscode": "^1.27.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:Dopi-export-node"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "Dopi-export",
					"title": "Dopi",
					"icon": "resources/device.svg"
				}
			]
		},
		"views": {
			"Dopi-export": [
				{
					"id": "Dopi-export-node",
					"name": "Debug"
				},
				{
					"id": "Dopi-examples-node",
					"name": "Examples"
				}
			]
		},
		"commands": [
			{
				"command": "dopi.search",
				"title": "Dopi Search device"
			},
			{
				"command": "dopi.connect",
				"title": "Dopi Connect device"
			},
			{
				"command": "dopi.disconnect",
				"title": "Dopi Disconnect device"
			},
			{
				"command": "dopi.run",
				"title": "Dopi Run Micropython"
			},
			{
				"command": "dopi.micropython_install",
				"title": "Micropython install library from github"
			},
			{
				"command": "dopi.stop",
				"title": "Dopi Stop Micropython"
			},
			{
				"command": "dopi.ui.update",
				"title": "Update"
			}
		]
		
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/cijliu/vscode-dopi-micropython"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.27.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.27.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
