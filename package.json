{
	"name": "dopi-micropython",
	"displayName": "dopi-micropython",
	"publisher": "Dopi-micropython",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.24.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
            "activitybar": [
                {
                    "id": "Dopi-export",
                    "title": "Dopi",
                    "icon": "resources/device.svg"
                }
            ]
		},
		"views": {
            "Dopi-export": [
                {
                    "id": "Dopi-export-node",
                    "name": "Dopi"
				}
            ]
		},
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "dopi.search",
				"title": "Dopi Search device"
			},
			{
				"command": "dopi.connect",
				"title": "Dopi Connect device"
			},
			{
				"command": "dopi.disconnect",
				"title": "Dopi Disconnect device"
			},
			{
				"command": "dopi.run",
				"title": "Dopi Run Micropython"
			},
			{
				"command": "dopi.stop",
				"title": "Dopi Stop Micropython"
			},
			{
				"command": "dopi.ui.update",
				"title": "Update"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
